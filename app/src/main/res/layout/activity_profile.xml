<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">


    <TextView
        android:id="@+id/textViewDays2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_below="@+id/textView13"
        android:layout_alignParentBottom="false"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:layout_weight="66.66"
        android:background="@drawable/btn_rounded_corners"
        android:backgroundTint="@color/colorAccent"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:text="00"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.921"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ContactAlter_TV_ID" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="132dp"
        android:layout_marginTop="-5dp"
        android:layout_marginBottom="8dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/header_contact" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="234dp"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/poppins_light"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/ContactName_TV_ID"
        android:layout_width="366dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/poppins_semibold"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:text="Beispiel Name"
        android:textColor="@color/colorText_darkRed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/ContactAlter_TV_ID"
        android:layout_width="49dp"
        android:layout_height="31dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="36dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Alter"
        android:textAlignment="textEnd"
        android:textColor="@color/colorText_darkRed"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toEndOf="@+id/contactDatepicker_TV_ID"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="234dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins_light"
        android:text="Geburtsdatum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ContactName_TV_ID" />

    <include
        android:id="@+id/horoscopeCard"
        layout="@layout/daily_horoscope_card"
        android:layout_width="374dp"
        android:layout_height="254dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="58dp"
        android:layout_height="22dp"
        android:layout_marginTop="8dp"
        android:text="Alter"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toTopOf="@+id/ContactAlter_TV_ID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.728"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/ContactName_TV_ID"
        app:layout_constraintVertical_bias="0.29" />

    <EditText
        android:id="@+id/contactDatepicker_TV_ID"
        android:layout_width="306dp"
        android:layout_height="51dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/poppins_semibold"
        android:hint="Select Date"
        android:inputType="date"
        android:textColor="@color/colorText_darkRed"
        android:textColorHint="@color/colorText_darkRed"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />


    <TextView
        android:id="@+id/starSign_TV_ID"
        android:layout_width="362dp"
        android:layout_height="37dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="22dp"
        android:fontFamily="@font/poppins_bold"
        android:foregroundTint="#00000000"
        android:text="Days left til your birthday"
        android:textAlignment="textStart"
        android:textColor="@color/colorText_darkRed"
        android:textColorHint="#FA030303"

        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/horoscopeCard"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactDatepicker_TV_ID" />

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_white"
        android:layout_width="411dp"
        android:layout_height="58dp"
        android:foregroundTint="#FFFFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:subtitleTextColor="#FFFFFFFF"
        app:titleTextColor="#FFFFFFFF" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="204dp"
        android:layout_height="41dp"
        android:text="Profile"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.439" />


</androidx.constraintlayout.widget.ConstraintLayout>